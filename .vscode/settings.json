{
	"[rust]": {
		"editor.defaultFormatter": "rust-lang.rust-analyzer",
		"editor.rulers": [100],
		"editor.wordWrap": "bounded"
	},
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.semanticHighlighting.enabled": true,
	"files.associations": {
		"*.rs": "rust"
	},
	"keyboard.touchbar.enabled": false,
	"repomix.runner.useBundleNameAsOutputName": true,
	"rust-analyzer.cargo.autoreload": true,
	"rust-analyzer.cargo.noDefaultFeatures": false,
	"rust-analyzer.check.command": "clippy",
	"rust-analyzer.check.extraArgs": ["--", "-D", "warnings"],
	"rust-analyzer.check.features": "all",
	"rust-analyzer.checkOnSave": true,
	"rust-analyzer.debug.buildBeforeRestart": true,
	"rust-analyzer.diagnostics.styleLints.enable": false,
	//"rust-analyzer.diagnostics.disabled": ["unresolved-proc-macro"],
	"rust-analyzer.imports.granularity.group": "module",
	"rust-analyzer.interpret.tests": true,
	"rust-analyzer.lens.enable": true,
	"rust-analyzer.lens.implementations.enable": true,
	"rust-analyzer.procMacro.enable": true,
	"rust-analyzer.restartServerOnConfigChange": true,
	"syntax.highlightComment": true,
	"repomix.output.includeEmptyDirectories": true,
	"repomix.output.compress": true,
	"debug.javascript.defaultRuntimeExecutable": {
		"pwa-node": "/Users/smeya/.local/share/mise/shims/node"
	}
}
